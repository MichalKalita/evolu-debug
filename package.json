{
  "name": "evolu-debug",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/main.ts",
      "import": "./dist/evolu-debug.js",
      "default": "./dist/evolu-debug.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:example": "vite --open /examples/shadow-dom/index.html",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:browser": "vitest --config vitest.browser.config.ts",
    "test:ci": "npm run type-check && npm run test:unit -- --run && npm run test:browser -- --run",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint . --fix",
    "lint:eslint": "eslint . --fix --cache",
    "format": "oxfmt src/"
  },
  "dependencies": {
    "@evolu/common": "^7.4.1",
    "@evolu/vue": "^1.4.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@evolu/web": "^2.4.0",
    "@tsconfig/node24": "^24.0.4",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.13",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "~1.46.0",
    "eslint-plugin-vue": "~10.8.0",
    "jiti": "^2.6.1",
    "jsdom": "^28.1.0",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.32.0",
    "oxlint": "~1.47.0",
    "playwright": "^1.58.2",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-css-injected-by-js": "^4.0.0",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
